<div class="well">
  <h3>Profile</h3>
  <h4><%= current_user.name %></h4>
  <h5><%= current_user.email %></h5>
</div>
<div class="well">
  <h3>Posts</h3>
<% @user_posts.each do |posts| %>
  <div>
  <a href="/">
    <strong><%= posts.title %></strong></a>
    <% posts.comment.each do |comments| %>
      <% if comments.user_id == current_user.id %>
        <p><%= comments.content %></p>
      <% end %>
    <% end %>
  </div>

<% end %>
</div>
